<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="JMF">

	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level [%t] %logger - %msg%n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>

</Configuration>